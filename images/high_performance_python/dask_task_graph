// Dask Task Graph
digraph {
	a [label="load a"]
	b [label="load b"]
	c [label="load c"]
	d [label="load d"]
	sum_ab [label="sum(a, b)"]
	sum_cd [label="sum(c, d)"]
	mul [label="mul(sum_ab, sum_cd)"]
	a -> b
	a -> c
	c -> d
	b -> d
	a -> sum_ab
	b -> sum_ab
	c -> sum_cd
	d -> sum_cd
	sum_ab -> mul
	sum_cd -> mul
}
