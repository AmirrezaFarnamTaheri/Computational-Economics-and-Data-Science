		A2 -> A3
		A3 -> A1
		label="Micro Level: Agent Interactions"
		A1 [label="Agent 1"]
		A2 [label="Agent 2"]
		A3 [label="Agent 3"]
	}
	B [label="Macro Level: Emergent Pattern (e.g., Market Crash)"]
	A1 -> B [style=dashed]
	A2 -> B [style=dashed]
	A3 -> B [style=dashed]
}
